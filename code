import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	String a ="y";
	while(a.equals("y")) {
		System.out.print("국어 영어 수학을 입력하세여");
		int kor = sc.nextInt();
		int eng = sc.nextInt();
		int math = sc.nextInt();
		Grade grade = new Grade(kor, eng, math);
		System.out.println("평균은 "+grade.avg());
        
        System.out.print("계속 하시겠습니까? y또는 n입력");
        a = sc.next();             
        
        if(a.equals("n"))   
      {  System.out.println("프로그램 종료입니다.");
        break; 
       }
	}
}
