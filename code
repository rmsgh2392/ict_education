      //한빈
      
      import java.util.Scanner;

class CountCurrent{
	private int m5,m1,c5,c1,b5,b1;
	
	private int money;
	
	public CountCurrent(int money) {
		this.money = money;
		this.m5 = money/50000;
		this.m1 = (money%50000)/10000;
		this.c5 = (money%10000)/5000;
		this.c1 = (money%5000)/1000;
		this.b5 = (money%1000)/500;
		this.b1 = (money%500)/100;
	}
	
	public void show() {
		System.out.println(money);
		System.out.println("오만원 : " + m5 + "장");
		System.out.println("만원 : " + m1 + "장");
		System.out.println("오천원 : " + c5 + "장");
		System.out.println("천원 : " + c1 + "장");
		System.out.println("오백원 : " + b5 + "개");
		System.out.println("백원 : " + b1 + "개");
	}
}
public class Practice01 {

	public static void main(String[] args) {
		for(int i = 1; true ; i++) {
		Scanner sc = new Scanner(System.in);
		System.out.println("금액을 입력하세요");
		int current = sc.nextInt();
		
		System.out.println(current + "원");
		
		CountCurrent count = new CountCurrent(current);
		count.show();
		
		System.out.println("계속 하시겠습니까?");
		String answer = sc.next();
		
		if(answer.equals("y") || answer.equals("Y")) {
			continue;
		} else {
			break;
		}
		
	}
  
  
  //혁진
  
  public class Money {
	public int total;
	public int temp;

	public void setMon(int total) {
		this.total = total;
	}

	public void count(){
		System.out.println("5만원:" + total/50000 +"장");
		temp = total%50000;
		System.out.println("만원:" + temp/10000 +"장");
		temp = temp%10000;
		System.out.println("5천원:" + temp/5000 +"장");
		temp = temp%5000;
		System.out.println("천원:" + temp/1000 +"장");
		temp = temp%1000;
		System.out.println("5백원:" + temp/500 +"장");
		temp = temp%500;
		System.out.println("백원:" + temp/100 +"장");
		temp = temp%100;
	}
}
import java.util.Scanner;

public class MoneyCount {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Money mon = new Money();
		int money;
		String str = null;
		
		for (int i = 1; i < 10; i++) {
			System.out.println("총액을 입력하세요");
			money = sc.nextInt();
			mon.setMon(money);
			mon.count();
			System.out.println("계속 하시겠습니까?");
			if(str.equals("y")) {
				continue;
			}else if(str.equals("n")) {
				System.out.println("프록램을 종료합니다.");
			}break;
		}
		
	}
}
//승환

import java.util.Scanner;

public class MoneyCount {

	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)) {
			Money mon = new Money();
			int money;
			String str;
			
			for(int i=0;i<10;i++) {
				System.out.println("돈을 입력하세요.");
				money = sc.nextInt();
				mon.setMon(money);
				mon.count();
				System.out.println("계속하시겠습니까");
				str = sc.next();
				if (str.equals("y")) {
					continue;
				} else if (str.equals("n")) {
					System.out.println("프로그램 종료 입니다.");
					break;
				}
				
			}
		}
		
	}

}
public class Money {
	private int mon;
	private int min;
	
	public void setMon(int mon) {
		this.mon = mon;
	}
	
	public void count() {
		System.out.println("오만원 : "+ mon/50000+"장");
		min = mon%50000;
		System.out.println("만원 : "+ min/10000+"장");
		min = min%10000;
		System.out.println("오천원 : "+ min/5000+"장");
		min = min%5000;
		System.out.println("천원 : "+ min/1000+"장");
		min = min%1000;
		System.out.println("오백원 : "+ min/500+"개");
		min = min%500;
		System.out.println("백원 : "+ min/100+"개");
	}
}

