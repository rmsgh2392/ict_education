		//
    while (true) {
			System.out.println("국어 영어 수학을 입력하세요!");
			kor = sc.nextInt();
			math = sc.nextInt();
			eng = sc.nextInt();

			System.out.println("평균은 : " + (kor + math + eng) / 3.0);

			System.out.println("계속 하시겠습니까?");
			ch = sc.next().charAt(0);

			if (ch == 'y')
				continue;
			else if (ch == 'n')
				break;
			else
				System.out.println("y 혹은 n만 입력해주세요");

		}
    
    
//승환    
    import java.util.Scanner;

public class GradeScanner {

	public static void main(String[] args) {

		int kor;
		int eng;
		int math;
		int total;
		double avg;
		String str;
		Scanner sc = new Scanner(System.in);

		for (int i = 0; i < 10; i++) {
			System.out.println("국어 영어 수학을 입력하세요!");
			kor = sc.nextInt();
			eng = sc.nextInt();
			math = sc.nextInt();
			total = kor + eng + math;
			avg = total / 3.0;
			System.out.println("평균은 " + avg);
			System.out.println("계속하시겠습니까");
			str = sc.next();
			if(str.equals("y")) {
				continue;
			}else if(str.equals("n")) {
				System.out.println("프로그램 종료 입니다.");
				break;
			}
		}

	}

}

//명진

import java.util.Scanner;

public class ScannerTest03 {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		for (int i = 0; ; i++) {

			System.out.print("국어 : ");
			int kor = sc.nextInt();
			System.out.print("영어 : ");
			int eng = sc.nextInt();
			System.out.print("수학 : ");
			int math = sc.nextInt();
			int total = kor + eng + math;
			double avg = (double) total / 3;
			System.out.println("평군은 " + avg);
			System.out.println("계속 하시겠습니까");
			
			String yesOrNo = sc.next();
			
				
			if(yesOrNo.equals ("y") || yesOrNo.equals("Y")){
				continue;
			}else
				System.out.println("프로그램 종료 입니다.");
			break;

		}

	}
}
//한빈
public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);

		for(int i = 1; true; i++) {
				System.out.println("국어, 수학, 영어를 입력하세요!");
				int num1 = sc.nextInt();
				int num2 = sc.nextInt();
				int num3 = sc.nextInt();
				
				double avg = (num1+num2+num3)/3.0;
				System.out.println("평균은 " + avg);
				
				System.out.println("계속 하시겠습니까");
				String answer = sc.next();
				System.out.println(answer);
				
				if(answer.equals("y") || answer.equals("Y")) {
					continue;
				}
				if(answer.equals("n") || answer.equals("N")) {
					System.out.println("프로그램 종료입니다.");
					break;
				}
				
			
			}
	}
  
  
